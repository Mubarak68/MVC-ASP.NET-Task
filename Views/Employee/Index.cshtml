
                @model List<Employee>
                <h1>Employee List</h1>
                <table class="table">
    <tr>
        <td>ID</td>
        <td>Fname</td>
        <td>Lname</td>
        <td>Email</td>
        <td>Department</td>
    </tr>
        @foreach (var employees in Model)
        {
        <tr>
            <td><a href="/Employee/Details/@employees.EmployeeId">@employees.EmployeeId</a></td>
            <td>@employees.FirstName</td>
            <td>@employees.LastName</td>
            <td>@employees.Email</td>
            <td>@employees.Department</td>
        </tr>
        }
    
</table>

<a href="/Employee/Register" class="btn btn-info">Add Employee</a>